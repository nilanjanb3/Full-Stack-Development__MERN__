<%- include("partials/header"); %>
    <h1>Compose</h1>


    <form action="/compose" method="post">
        <div class="form-group">
            <label>Title</label><br>
            <input type="text" name="postTitle" placeholder="Type here..." class="form-control">
            <label>Post</label>
            <textarea name="postBody" rows="5" placeholder="Write your post here..." class="form-control"></textarea>
        </div>
        <button class="btn btn-primary" type="submit">Publish</button>

    </form>

    <%- include("partials/footer"); %>